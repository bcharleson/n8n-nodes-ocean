    "nodes": [
      "dist/nodes/Ocean/Ocean.node.js"
    ]
  },
  "devDependencies": {
    "typescript": "^5.5.3",
    "gulp": "^4.0.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
